{
	"name":"form_param",
	"caption":"",
	"type":"accordion",
	"subType":"default",
	"className":["sevian","horizontal"],
	"autoClose":false,
	"items":[
		{
			"caption":"Guardar Parámetros",
			"events":{
				"click":"this.getContext().save(0);"
			}
		},
		{
			"caption":"Configurar",
			"events":{
				"click":"this.getContext().s('SET');"
			}

			
		},
		{
			"caption":"Leer Configuración ",
			"events":{
				"click":"this.getContext().paramReadLoad(this.getContext().getCommandId())"
			}

			
		},
		{
			"caption":"Cancel",
			"events":{
				"click":"this.getContext().loadHistory()"
			}
		}
		
	]
}	